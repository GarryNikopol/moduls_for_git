
print("Проверяем целые числа на совпадение!")

x1 = int(input("Ведите первое целое число: "))
x2 = int(input("Ведите второе целое число: "))
x3 = int(input("Ведите третье целое число: "))
matcheck1 = (x1==x2==x3) * 1
matcheck2 = (x3!=x2==x1) * 1 | (x1!=x2==x3) * 1 | (x2!=x1==x3) * 1


if(matcheck1==1):
	print(3)
elif(matcheck2==1):
	print(2)
else:
	print(0)